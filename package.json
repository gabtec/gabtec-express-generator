{
	"name": "@gabtec/express-generator",
	"description": "A cli tool to create express.js code",
	"version": "1.0.2",
	"author": "Gabriel Martins",
	"main": "lib/index.js",
	"bin": {
		"gegen": "cli.js"
	},
	"scripts": {
		"t": "jest ",
		"test": "jest --verbose --silent",
		"test:w": "jest --verbose --silent --watchAll",
		"test:cov": "jest --silent --coverage",
		"t:ls": "jest --listTests"
	},
	"keywords": [
		"node",
		"js",
		"express",
		"rest",
		"terminal",
		"generator"
	],
	"license": "MIT",
	"dependencies": {
		"figlet": "^1.5.2"
	},
	"devDependencies": {
		"@types/jest": "^29.0.3",
		"colors": "^1.4.0",
		"jest": "^29.0.3"
	}
}
